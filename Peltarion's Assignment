        "id": "KPKWa_So1ynp",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "data = os.path.join('/content','data')\n",
        "\n",
        "\n",
        "lesion_type_dict = {\n",
        "    'nv': 'Melanocytic nevi',\n",
        "    'mel': 'Melanoma',\n",
        "    'bkl': 'Benign keratosis-like lesions ',\n",
        "    'bcc': 'Basal cell carcinoma',\n",
        "    'akiec': 'Actinic keratoses',\n",
        "    'vasc': 'Vascular lesions',\n",
        "    'df': 'Dermatofibroma'\n",
        "}\n",
        "image_extension = '*.jpg'\n",
        "paths = sorted((y for x in os.walk(data) for y in glob (os.path.join(x[0], image_extension))))\n",
        "\n",
        "final_dict = {os.path.splitext(os.path.basename(x))[0]: x for x in paths}"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "na0zBRtI106k",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "dataframe = pd.read_csv('HAM10000.csv')\n",
        "\n",
        "# Creating New Columns for better readability\n",
        "\n",
        "dataframe['path'] = dataframe['image_id'].map(final_dict.get)\n",
        "dataframe['cell_type'] = dataframe['dx'].map(lesion_type_dict.get) \n",
        "dataframe['cell_type_idx'] = pd.Categorical(dataframe['cell_type']).codes"
      ],
      "execution_count": 0,
